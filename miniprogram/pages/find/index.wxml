<!--pages/find/index.wxml-->
<view class="find">
  <van-tabs type="card" color="#165DFF" swipeable>
    <van-tab title="预约日历">
      <van-calendar title="预约日历" poppable="{{ false }}" color="#165DFF" show-confirm="{{ false }}" class="calendar" formatter="{{ formatter }}" />
    </van-tab>
    <van-tab title="学习报告">
      <view class="container">
        <ec-canvas id="mychart-dom-line" force-use-old-canvas="true" canvas-id="mychart-line" ec="{{ ec }}"></ec-canvas>

      </view>
    </van-tab>
    <van-tab title="排行榜">

    </van-tab>
  </van-tabs>
</view>