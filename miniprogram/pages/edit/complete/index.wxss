/* pages/edit/complete/index.wxss */
.container {
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-flow: column nowrap;
  overflow: hidden;
  background-color: #fff;
}

.container-content {
  width: 100%;
  /* height: 100%; */
  margin-top: 10vh;
}

.container-content-img {
  margin: 50rpx 0;
  display: flex;
  justify-content: center;
}

.img-msg {
  text-align: center;
  font-size: 12px;
  color: #ddd;
}


.msg {
  margin-top: 50rpx;
  text-align: center;
}

.icon_complete {
  font-size: 80rpx;
  color: #2c2c22;
}

.bottom {
  margin: 0 auto;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  bottom: 40px;
}

.bottom button:first-child {
  width: 40%;
  height: 55px;
  margin-left: 30px;
  background-color: #eee;
  border-radius: 10px;
  font-size: 18px;
  text-align: center;
  line-height: 55px;
}

.bottom button:last-child {
  width: 40%;
  height: 55px;
  margin-right: 30px;
  background-color: #2c2c2c;
  border-radius: 10px;
  color: #fff;
  font-size: 18px;
  text-align: center;
  line-height: 55px
}